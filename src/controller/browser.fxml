<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="700.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <ScrollPane fitToHeight="true" fitToWidth="true" layoutX="5.0" layoutY="10.0" prefHeight="700" >
               <content>
                  <VBox>
                     <padding>
                        <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
                     </padding>
                     <children>
                        <VBox>
                           <children>
                              <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome : &lt;User&gt;" textAlignment="CENTER" wrappingWidth="958.921875">
                                 <font>
                                    <Font size="20.0" />
                                 </font>
                              </Text>
                              <Separator prefHeight="30.0" />
                           </children>
                        </VBox>
                        <VBox minHeight="500.0" fx:id="browser" />
                        <VBox>
                           <children>
                              <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Please specify Beliverability" textAlignment="CENTER" wrappingWidth="957.1796875">
                                 <font>
                                    <Font size="14.0" />
                                 </font>
                                 <VBox.margin>
                                    <Insets bottom="10.0" top="10.0" />
                                 </VBox.margin>
                              </Text>
                              <Text  strokeType="OUTSIDE" strokeWidth="0.0" text="Value : 0" fx:id="browser_slider_text1">
                                 <VBox.margin>
                                    <Insets bottom="10.0" top="10.0" />
                                 </VBox.margin>
                              </Text>
                              <Slider fx:id="browser_slider1" showTickLabels="true" showTickMarks="true"  >
                                 <VBox.margin>
                                    <Insets bottom="10.0" top="10.0" />
                                 </VBox.margin>
                              </Slider>
                              <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Please specify your knowledge" textAlignment="CENTER" wrappingWidth="955.439453125">
                                 <font>
                                    <Font size="14.0" />
                                 </font>
                                 <VBox.margin>
                                    <Insets bottom="10.0" top="10.0" />
                                 </VBox.margin>
                              </Text>
                              <Text strokeType="OUTSIDE" strokeWidth="0.0" text=" Value : 0" fx:id="browser_slider_text2">
                                 <VBox.margin>
                                    <Insets bottom="10.0" top="10.0" />
                                 </VBox.margin>
                              </Text>
                              <Slider fx:id="browser_slider2" showTickLabels="true" showTickMarks="true"  >
                                 <VBox.margin>
                                    <Insets bottom="10.0" top="10.0" />
                                 </VBox.margin>
                              </Slider>
                              <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Please leave your comments" textAlignment="CENTER" wrappingWidth="955.7880859375">
                                 <font>
                                    <Font size="14.0" />
                                 </font>
                                 <VBox.margin>
                                    <Insets bottom="10.0" top="10.0" />
                                 </VBox.margin>
                              </Text>
                              <TextArea prefHeight="300.0" prefWidth="200.0">
                                 <VBox.margin>
                                    <Insets bottom="10.0" top="10.0" />
                                 </VBox.margin>
                              </TextArea>
                              <Button fx:id="browser_submit" mnemonicParsing="false" prefHeight="30.0" prefWidth="959.0" text="Submit">
                                 <font>
                                    <Font size="14.0" />
                                 </font>
                              </Button>
                           </children>
                        </VBox>
                     </children>
                  </VBox>
               </content>
            </ScrollPane>
         </children>
    </AnchorPane>
  </children>
</VBox>
